{
  "version": "1.0.0-*",
  "testRunner": "xunit",
  "dependencies": {
    "NETStandard.Library": "1.6.1",
    "dotnet-test-xunit": "2.2.0-*",
    "Microsoft.AspNetCore.Testing": "1.2.0-*",
    "xunit": "2.2.0-*",
    "Microsoft.Extensions.FileProviders.Composite": "1.0.0",
    "Microsoft.Extensions.FileProviders.Embedded": "1.0.0",
    "Microsoft.AspNetCore.NodeServices": "1.0.0-beta-000007",
    "Microsoft.AspNetCore.TestHost": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Dazinator.AspNet.Extensions.FileProviders": { "target": "project" }
  },
  "buildOptions": {
    "copyToOutput": "TestDir\\",
    "embed": "Resources/*.*"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-*",
          "type": "platform"
        }
      },
      "buildOptions": { "define": [ "NETSTANDARD" ] }
    }
   
  }
}
